package jpr;

public class Main {
 public static void main(String[] args) {
        
        //Byte Max Min Size 
        
        System.out.println("\nByte maxvalue: "+Byte.MAX_VALUE+ "\tByte Minvalue:"+Byte.MIN_VALUE+ "\tByte size"+Byte.SIZE);
        
         //ShortMax Min Size 
         
        System.out.println("\nShort maxvalue: "+Short.MAX_VALUE+"\tShortMinvalue: "+Short.MIN_VALUE+"\tShort size: "+Short.SIZE);
        	
         //Integer Max Min Size 
        System.out.println("\nInteger maxvalue:: "+Integer.MAX_VALUE+" \tInteger Minvalue:"+Integer.MIN_VALUE+"\tnInteger size "+Integer.SIZE);
        
         //Float Max Min Size
          
        System.out.println("\nFloat maxvalue: "+Float.MAX_VALUE+"\t FloatMinvalue:"+Float.MIN_VALUE+" \tFloatsize"+Float.SIZE);
        
         //Doublee Max Min Size 
         
        System.out.println("\nDouble maxvalue: "+Double.MAX_VALUE+"\tDouble Minvalue:"+Double.MIN_VALUE+"\tDouble size:"+Double.SIZE);
        
         //Character Max Min Size 
         
        System.out.println("\nCharacter maxvalue:"+Character.MAX_VALUE+"\tCharacterMinvalue: "+Character.MIN_VALUE+"\tCharacter size: "+Character.SIZE);

    }
}
